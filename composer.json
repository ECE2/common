{
    "name": "ece2/common",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "autoload": {
        "psr-4": {
            "Ece2\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "hyperf/amqp": "^2.2",
        "hyperf/async-queue": "^2.2",
        "hyperf/circuit-breaker": "^2.2",
        "hyperf/config-center": "^2.2",
        "hyperf/config-nacos": "^2.2",
        "hyperf/constants": "^2.2",
        "hyperf/database": "^2.2",
        "hyperf/db-connection": "~2.2.0",
        "hyperf/http-server": "~2.2.0",
        "hyperf/json-rpc": "^2.2",
        "hyperf/model-cache": "^2.2",
        "hyperf/paginator": "^2.2",
        "hyperf/rate-limit": "^2.2",
        "hyperf/redis": "^2.2",
        "hyperf/retry": "^2.2",
        "hyperf/rpc": "^2.2",
        "hyperf/rpc-client": "^2.2",
        "hyperf/rpc-server": "^2.2",
        "hyperf/service-governance": "^2.2",
        "hyperf/service-governance-nacos": "^2.2",
        "hyperf/tracer": "^2.2",
        "hyperf/watcher": "^2.2",
        "symfony/var-dumper": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "^4.5"
    },
    "suggest": {
        "swow/swow": "Required to create swow components."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Ece2\\Common\\ConfigProvider"
        }
    }
}
